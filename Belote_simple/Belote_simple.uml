        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uQum0K1ZEeOGC_9tRImheg" name="calculerValeurCarte" specification="_uPi7EK1ZEeOGC_9tRImheg">
          <ownedParameter xmi:id="_uQum0a1ZEeOGC_9tRImheg" name="atout" type="_uMaHgK1ZEeOGC_9tRImheg"/>
          <ownedParameter xmi:id="_uQum0q1ZEeOGC_9tRImheg" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>int point = 0;&#xD;
		switch (this.figure) {&#xD;
		case Neuf:&#xD;
			if(atout.equals(this.getCouleur())) {&#xD;
				point = 14;&#xD;
			}else{&#xD;
				point = 0;&#xD;
			}&#xD;
			break;&#xD;
		case Dix:&#xD;
			point = 10;&#xD;
			break;&#xD;
		case Valet:&#xD;
			if(atout.equals(this.getCouleur())) {&#xD;
				point = 20;&#xD;
			}else{&#xD;
				point = 2;&#xD;
			}&#xD;
			break;&#xD;
		case Dame:&#xD;
			point = 3;&#xD;
			break;&#xD;
		case Roi:&#xD;
			point = 4;&#xD;
			break;&#xD;
