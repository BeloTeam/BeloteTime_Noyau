          <body>int numCarte = 0;&#xD;
		String res = &quot;(&quot;;&#xD;
		Set&lt;CouleurEnum> keys = this.main.keySet();&#xD;
		for (Iterator&lt;CouleurEnum> it = keys.iterator(); it.hasNext();) {&#xD;
			CouleurEnum key = (CouleurEnum) it.next();&#xD;
			for (Iterator&lt;Carte> it2 = this.main.get(key).iterator(); it2.hasNext();) {&#xD;
