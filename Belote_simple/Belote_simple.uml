          <language>java</language>
          <body>Equipe e;&#xD;
		if (joueur.getPosition() == PositionEnum.Nord || joueur.getPosition() == PositionEnum.Sud)&#xD;
			e = this.equipes.get(0);&#xD;
		else &#xD;
			e = this.equipes.get(1);&#xD;
		return e;&#xD;
	</body>
        </ownedBehavior>
