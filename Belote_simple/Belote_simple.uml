          <ownedParameter xmi:id="_uQxqJK1ZEeOGC_9tRImheg" type="_uMoxAK1ZEeOGC_9tRImheg" direction="return"/>
          <language>java</language>
          <body>// Initialisation du tas&#xD;
		for (CouleurEnum c : CouleurEnum.values()) {&#xD;
			for (FigureEnum f : FigureEnum.values()) {&#xD;
				this.ajouter(new Carte(c, f));&#xD;
			}&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uQxqJa1ZEeOGC_9tRImheg" name="couper" specification="_uMpYFK1ZEeOGC_9tRImheg">
          <ownedParameter xmi:id="_uQxqJq1ZEeOGC_9tRImheg" name="pourcentageCartesCoupees">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <ownedParameter xmi:id="_uQxqJ61ZEeOGC_9tRImheg" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
          </ownedParameter>
          <language>java</language>
          <body>int nbCarteCoupees = 0;&#xD;
		boolean estCoupe = false;&#xD;
		if (pourcentageCartesCoupees >= 0 &amp;&amp; pourcentageCartesCoupees &lt;= 100) {&#xD;
			estCoupe = true;&#xD;
			nbCarteCoupees = (int) (31 * (pourcentageCartesCoupees / 100));&#xD;
			for (int i = 0; i &lt; nbCarteCoupees; i++) {&#xD;
				Carte c = super.getCartes().remove(i);&#xD;
				estCoupe &amp;= super.getCartes().add(c);&#xD;
			}&#xD;
		}&#xD;
		return estCoupe;&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_uQxqKK1ZEeOGC_9tRImheg" name="melanger" specification="_uMp_IK1ZEeOGC_9tRImheg">
          <ownedParameter xmi:id="_uQxqKa1ZEeOGC_9tRImheg" name="nbTentativeSwitch">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>for (int k = 0; k &lt; nbTentativeSwitch; k++) {&#xD;
			int i = (int) (Math.random() * (32 - 1));&#xD;
			int j = (int) (Math.random() * (32 - 1));&#xD;
			Carte c = super.getCartes().remove(i);&#xD;
			super.getCartes().add(j, c);&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
